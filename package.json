{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ssr": "tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --config vite.config.ssr.ts --outDir dist/server",
    "build:netlify": "npm run build && echo 'Copying all files...' && cp -r public/* dist/client/ && echo 'Ensuring index.html exists...' && ls -la dist/client/index.html && echo 'Copying _redirects...' && cp public/_redirects dist/client/_redirects && echo 'Checking _redirects content...' && cat dist/client/_redirects && echo 'Build complete!'",
    "build:dev": "vite build --mode development",
    "serve": "NODE_ENV=production node dist/server/entry-server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:data": "tsc src/components/serviceAreaData.ts --outDir scripts/data --module NodeNext --target ES2020",
    "generate-sitemap": "echo \"Please run the command from the instructions and paste your keys directly.\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "@types/express": "^5.0.3",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "lovable-tagger": "^1.1.9",
    "lucide-react": "^0.344.0",
    "marked": "^16.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.3",
    "swiper": "^11.0.5",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.4",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@netlify/functions": "^4.2.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "front-matter": "^4.0.2",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
